<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Entity.UsersInfoEntity" table="users_info" schema="knowledge_sharing">
        <id name="userInfoId">
            <column name="userInfoId" sql-type="int(255)"/>
        </id>
        <property name="userId">
            <column name="userId" sql-type="int(255)" not-null="true"/>
        </property>
        <property name="userAccount">
            <column name="userAccount" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="userSex">
            <column name="userSex" sql-type="smallint(6)" not-null="true"/>
        </property>
        <property name="userbirthday">
            <column name="userbirthday" sql-type="date" not-null="true"/>
        </property>
        <property name="userIntroduction">
            <column name="userIntroduction" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="userRegisterTime">
            <column name="userRegisterTime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="userImg">
            <column name="userImg" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="token">
            <column name="token" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="userReward">
            <column name="userReward" sql-type="float(255)" precision="255" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>