<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Entity.QuestionTypeEntity" table="question_type" schema="knowledge_sharing">
        <id name="questionTypeId">
            <column name="questionTypeId" sql-type="int(255)"/>
        </id>
        <property name="questionType">
            <column name="questionType" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>